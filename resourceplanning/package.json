    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.3.0",
    "jasmine": "^2.4.1",
    "jasmine-reporters": "2.0.0",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "karma": "0.13.21",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-html-reporter": "^0.2.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter-livereload": "^1.0.0",
    "karma-junit-reporter": "0.3.8",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-requirejs": "^0.2.3",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
