{
  "name": "vbcm-website",
  "version": "0.0.1",
  "description": "vbcm website using angular, angular material",
  "main": "webpack.config.js",
  "scripts": {
    "#dev": "Same as <dev-full> but does not bundle vendors.js. It uses a vendors.js which has previously been built. This is the favorite profile for development",
    "dev": "webpack --watch --hot --progress --colors --config webpack.config.js",
    "#dev-full": "Running with browsersync on port 3000, 3rd party libraries will be bundled into vendor.js",
    "dev-full": "webpack --watch --hot --progress --colors --config webpack.config.js",
    "prod": "webpack -p --progress --colors --config webpack.config.js",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "pree2e-test": "npm run update-webdriver",
    "e2e-test": "protractor src/test/e2e-tests/protractor.conf.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "browser-sync": "^2.11.0",
    "browser-sync-webpack-plugin": "^1.0.1",
